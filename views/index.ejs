    <section id="fh5co-contact" data-section="Connexion">
      <div class="container">
        <% if (!user) { %>
        <div class="col-md-6 to-animate">
          <h3>Connexion</h3>
          <form action="/login" method="post">
          <div class="form-group ">
            <label for="name" class="sr-only">Nom d'utilisateur</label>
            <input name="username" id="name" class="form-control" placeholder="Nom d'utilisateur" type="text">
          </div>
          <div class="form-group ">
            <label for="email" class="sr-only">Mot de passe</label>
            <input name="password" id="email" class="form-control" placeholder="Mot de passe" type="password">
          </div>
          <div class="form-group ">
            <input class="btn btn-primary btn-lg" style="width:100%;" value="Connexion" type="submit">
          </div>
          </form>
        </div>
        <div class="col-md-6 to-animate">
          <h3>Inscription</h3>
        <form role="form" action="/register" method="post" style="max-width: 300px;" class="form-signin">
          <input type="text" name="name" placeholder="Votre Nom Prénom" class="form-control">
          <input type="text" name="username" placeholder="Identifiant" class="form-control">
          <input type="password" name="password" placeholder="Mot de passe" class="form-control">
          <button type="submit" class="btn btn-lg btn-primary btn-block">Sign Up</button>
        </form>
      </div>
        <% } else { %>
  <p>Hello, <%= user.name %>. View your <a href="/profile">profile</a>.</p>
  <% } %>
